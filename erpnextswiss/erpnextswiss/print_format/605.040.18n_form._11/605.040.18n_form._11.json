 "format_data": "[{\"fieldname\": \"print_heading_template\", \"fieldtype\": \"Custom HTML\", \"options\": \"<style>\\n.form11 {\\n   background-image: url(\\\"/assets/erpnextswiss/images/605_040_18N_Form_11.png\\\");\\n   background-size:cover;\\n   background-repeat: no;\\n   height: 230mm;\\n   width: 160mm;\\n   /* margin-top: -10mm; \\n}\\n</style>\\n\\n{% set employee = frappe.get_doc(\\\"Employee\\\", doc.employee) %}\\n\\n<div class=\\\"form11\\\">\\n<!-- A/B -->\\n<table style=\\\"margin-left: 15mm; \\\">\\n  <tr>\\n    <td style=\\\"padding: 2px !important; margin-left: 5px; \\\">\\n      {% if doc.certificate_type == \\\"Salary\\\" %}X\\n      {% else %}&nbsp;{% endif %}\\n    </td>\\n  </tr>\\n  <tr>\\n    <td style=\\\"padding: 2px !important; margin-left: 5px; \\\">\\n      {% if doc.certificate_type == \\\"Pension\\\" %}X\\n      {% else %}&nbsp;{% endif %}\\n    </td>\\n  </tr>\\n</table>\\n<!-- C -->\\n<table style=\\\"margin-left: 15mm; margin-top: -2px; \\\">\\n  <tr>\\n    <td style=\\\"padding: 5px !important; width: 40mm; \\\">\\n      {{ employee.old_social_security_number }}\\n    </td>\\n    <td style=\\\"padding: 5px !important; width: 43mm; \\\">\\n      {{ employee.social_security_number }}\\n    </td>\\n    <td style=\\\"padding: 5px !important; \\\">\\n      {% if doc.free_transport == 1 %}X\\n      {% else %}&nbsp;{% endif %}\\n    </td>\\n  </tr>\\n</table>\\n<!-- D -->\\n<table style=\\\"margin-left: 15mm; margin-top: 2px; \\\">\\n  <tr>\\n    <td style=\\\"padding: 5px !important; width: 28mm; \\\">\\n      {{ doc.year }}\\n    </td>\\n    <td style=\\\"padding: 5px !important; width: 20mm; \\\">\\n      {{ frappe.utils.get_datetime(doc.start_date).strftime('%d.%m.')  }}\\n    </td>\\n    <td style=\\\"padding: 5px !important; width: 35mm; \\\">\\n      {{ frappe.utils.get_datetime(doc.end_date).strftime('%d.%m.') }}\\n    </td>\\n    <td style=\\\"padding: 5px !important; \\\">\\n      {% if doc.lunch_checks  == 1 %}X{% else %}&nbsp;{% endif %}\\n    </td>\\n  </tr>\\n</table>\\n\\n<table style=\\\"margin-left: 20mm; margin-top: 15mm; \\\">\\n  <tr>\\n    <td style=\\\"padding: 2px !important; \\\">\\n      {{ employee.employee_name }}<br>\\n      {{ employee.permanent_address.replace(\\\"\\\\n\\\", \\\"<br>\\\") }}\\n    </td>\\n  </tr>\\n</table> \\n</div>\"}, {\"fieldtype\": \"Section Break\", \"label\": \"\"}, {\"fieldtype\": \"Column Break\"}, {\"print_hide\": 0, \"fieldname\": \"_custom_html\", \"fieldtype\": \"HTML\", \"label\": \"Custom HTML\"}]", 
 "html": "{% set employee = frappe.get_doc(\"Employee\", doc.employee) %}\n\n<!-- A/B -->\n<table style=\"margin-left: 70px; height: 24px; \">\n  <tr>\n    <td style=\"padding-top: 4px !important; padding-bottom: 2px !important;\">\n      {% if doc.certificate_type == \"Salary\" %}X\n      {% else %}&nbsp;{% endif %}\n    </td>\n  </tr>\n</table>\n<table style=\"margin-left: 70px; margin-top: 2px; height: 24px; \">\n  <tr>\n    <td style=\"padding-top: 3px !important; padding-bottom: 2px !important;\">\n      {% if doc.certificate_type == \"Pension\" %}X\n      {% else %}&nbsp;{% endif %}\n    </td>\n  </tr>\n</table>\n<!-- C -->\n<table style=\"margin-left: 70px; margin-top: 2px; height: 24px; \">\n  <tr>\n    <td style=\"width: 185px; padding-top: 8px !important; \">\n      {{ employee.old_social_security_number }}\n    </td>\n    <td style=\"width: 225px; padding-top: 8px !important; \">\n      {{ employee.social_security_number }}\n    </td>\n    <td style=\"padding-top: 8px !important; \">\n      {% if doc.free_transport == 1 %}X\n      {% else %}&nbsp;{% endif %}\n    </td>\n  </tr>\n</table>\n<!-- D -->\n<table style=\"margin-left: 70px; margin-top: 10px; height: 24px; \">\n  <tr>\n    <td style=\"padding-left: 3px !important; padding-top: 4px !important; padding-bottom: 1px !important; width: 145px; \">\n      {{ doc.year }}\n    </td>\n    <td style=\"padding-left: 3px !important; padding-top: 4px !important; padding-bottom: 1px !important; width: 100px; \">\n      {{ frappe.utils.get_datetime(doc.start_date).strftime('%d.%m.')  }}\n    </td>\n    <td style=\"padding-left: 3px !important; padding-top: 4px !important; padding-bottom: 1px !important; width: 165px; \">\n      {{ frappe.utils.get_datetime(doc.end_date).strftime('%d.%m.') }}\n    </td>\n    <td style=\"padding-left: 3px !important; padding-top: 4px !important; padding-bottom: 1px !important; \">\n      {% if doc.lunch_checks  == 1 %}X{% else %}&nbsp;{% endif %}\n    </td>\n  </tr>\n</table>\n\n<table style=\"margin-left: 70px; margin-top: 60px; height: 150px; \">\n  <tr>\n    <td style=\"padding-left: 3px !important; padding-top: 4px !important; \">\n      {{ employee.employee_name }}<br>\n      {{ employee.permanent_address.replace(\"\\n\", \"<br>\") }}\n    </td>\n  </tr>\n</table> \n\n<!-- MAIN -->\n<table style=\"margin-left: 70px; width: 680px; margin-top: 75px; height: 24px; \">\n  <!-- 1 -->\n  <tr>\n    <td style=\"width: 580px; padding: 1px !important; \">\n    <td style=\"padding: 1px !important; \">{{ doc.salary }}</td>\n  </tr>\n</table>\n<table style=\"margin-left: 70px; width: 680px; margin-top: 2px; height: 24px; \">\n  <!-- 2.1 -->\n  <tr>\n    <td style=\"width: 580px; padding: 1px !important; \">\n    <td style=\"padding: 1px !important; \">{{ doc.catering }}</td>\n  </tr>\n</table>\n<table style=\"margin-left: 70px; width: 680px; margin-top: 2px; height: 24px; \">\n  <!-- 2.2 -->\n  <tr>\n    <td style=\"width: 580px; padding: 1px !important; \">\n    <td style=\"padding: 1px !important; \">{{ doc.car }}</td>\n  </tr>\n</table>\n<table style=\"margin-left: 70px; width: 680px; margin-top: 2px; height: 24px; \">\n  <!-- 2.3 -->\n  <tr>\n    <td style=\"width: 280px; padding: 1px !important; \"></td>\n    <td style=\"width: 300px; padding: 1px !important; \">{{ doc.other_salary_description or '&nbsp;' }}</td>\n    <td style=\"padding: 1px !important; \">{{ doc.other_salary }}</td>\n  </tr>\n</table>\n<table style=\"margin-left: 70px; width: 680px; margin-top: 25px; height: 24px; \">\n  <!-- 3 -->\n  <tr>\n    <td style=\"width: 580px; padding: 1px !important; \">{{ irregular_description or '&nbsp;' }}</td>\n    <td style=\"padding: 1px !important; \">{{ doc.irregular }}</td>\n  </tr>\n</table>\n<table style=\"margin-left: 70px; width: 680px; margin-top: 2px; height: 24px; \">\n  <!-- 4 -->\n  <tr>\n    <td style=\"width: 320px; padding: 1px !important; \"></td>\n    <td style=\"width: 260px; padding: 1px !important; \">{{ doc.capital_description or '&nbsp;' }}</td>\n    <td style=\"padding: 1px !important; \">{{ doc.capital }}</td>\n  </tr>\n</table>\n<table style=\"margin-left: 70px; width: 680px; margin-top: 2px; height: 24px; \">\n  <!-- 5 -->\n  <tr>\n    <td style=\"width: 580px; padding: 1px !important; \"></td>\n    <td style=\"padding: 1px !important; \">{{ doc.participation }}</td>\n  </tr>\n</table>\n<table style=\"margin-left: 70px; width: 680px; margin-top: 2px; height: 24px; \">\n  <!-- 6 -->\n  <tr>\n    <td style=\"width: 580px; padding: 1px !important; \"></td>\n    <td style=\"padding: 1px !important; \">{{ doc.board }}</td>\n  </tr>\n</table>\n<table style=\"margin-left: 70px; width: 680px; margin-top: 2px; height: 24px; \">\n  <!-- 7 -->\n  <tr>\n    <td style=\"width: 280px; padding: 1px !important; \"></td>\n    <td style=\"width: 300px; padding: 1px !important; \">{{ doc.other_description or '&nbsp;' }}</td>\n    <td style=\"padding: 1px !important; \">{{ doc.other }}</td>\n  </tr>\n</table>\n<table style=\"margin-left: 70px; width: 680px; margin-top: 4px; height: 24px; \">\n  <!-- 8 -->\n  <tr>\n    <td style=\"width: 580px; padding: 1px !important; \"></td>\n    <td style=\"padding: 1px !important; \">{{ doc.gross_salary }}</td>\n  </tr>\n</table>\n<table style=\"margin-left: 70px; width: 680px; margin-top: 2px; height: 24px; \">\n  <!-- 9 -->\n  <tr>\n    <td style=\"width: 580px; padding: 1px !important; \"></td>\n    <td style=\"padding: 1px !important; \">{{ doc.deduction_ahv }}</td>\n  </tr>\n</table>\n<table style=\"margin-left: 70px; width: 680px; margin-top: 2px; height: 24px; \">\n  <!-- 10.1 -->\n  <tr>\n    <td style=\"width: 580px; padding: 1px !important; \"></td>\n    <td style=\"padding: 1px !important; \">{{ doc.deduction_pension }}</td>\n  </tr>\n</table>\n<table style=\"margin-left: 70px; width: 680px; margin-top: 2px; height: 24px; \">\n  <!-- 10.2 -->\n  <tr>\n    <td style=\"width: 580px; padding: 1px !important; \"></td>\n    <td style=\"padding: 1px !important; \">{{ doc.deduction_pension_additional }}</td>\n  </tr>\n</table>\n<table style=\"margin-left: 70px; width: 680px; margin-top: 2px; height: 24px; \">\n  <!-- 11 -->\n  <tr>\n    <td style=\"width: 580px; padding: 1px !important; \"></td>\n    <td style=\"padding: 1px !important; \">{{ doc.net_salary }}</td>\n  </tr>\n</table>\n<table style=\"margin-left: 70px; width: 680px; margin-top: 16px; height: 24px; \">\n  <!-- 12 -->\n  <tr>\n    <td style=\"width: 580px; padding: 1px !important; \"></td>\n    <td style=\"padding: 1px !important; \">{{ doc.source_tax_deduction }}</td>\n  </tr>\n</table>\n<table style=\"margin-left: 70px; width: 680px; margin-top: 24px; height: 24px; \">\n  <!-- 13.1.1 -->\n  <tr>\n    <td style=\"width: 560px; padding: 1px !important; \"></td>\n    <td style=\"width: 20px; padding: 1px !important; \"></td>\n    <td style=\"padding: 1px !important; \">{{ doc.effective_expenses_travel }}</td>\n  </tr>\n</table>\n<table style=\"margin-left: 70px; width: 680px; margin-top: 2px; height: 24px; \">\n  <!-- 13.1.2 -->\n  <tr>\n    <td style=\"width: 280px; padding: 1px !important; \"></td>\n    <td style=\"width: 300px; padding: 1px !important; \">{{ doc.effective_other_expenses_description or '&nbsp;'}}</td>\n    <td style=\"padding: 1px !important; \">{{ doc.effective_other_expenses }}</td>\n  </tr>\n</table>\n<table style=\"margin-left: 70px; width: 680px; margin-top: 2px; height: 24px; \">\n  <!-- 13.2.1 -->\n  <tr>\n    <td style=\"width: 580px; padding: 1px !important; \"></td>\n    <td style=\"padding: 1px !important; \">{{ doc.representation_expenses }}</td>\n  </tr>\n</table>\n<table style=\"margin-left: 70px; width: 680px; margin-top: 2px; height: 24px; \">\n  <!-- 13.2.2 -->\n  <tr>\n    <td style=\"width: 580px; padding: 1px !important; \"></td>\n    <td style=\"padding: 1px !important; \">{{ doc.car_expenses }}</td>\n  </tr>\n</table>\n<table style=\"margin-left: 70px; width: 680px; margin-top: 2px; height: 24px; \">\n  <!-- 13.2.3 -->\n  <tr>\n    <td style=\"width: 280px; padding: 1px !important; \"></td>\n    <td style=\"width: 300px; padding: 1px !important; \">{{ doc.other_net_expenses_description or '&nbsp;'}}</td>\n    <td style=\"padding: 1px !important; \">{{ doc.other_net_expenses }}</td>\n  </tr>\n</table>\n<table style=\"margin-left: 70px; width: 680px; margin-top: 2px; height: 24px; \">\n  <!-- 13.3 -->\n  <tr>\n    <td style=\"width: 580px; padding: 1px !important; \"></td>\n    <td style=\"padding: 1px !important; \">{{ doc.education }}</td>\n  </tr>\n</table>\n<table style=\"margin-left: 70px; width: 680px; margin-top: 2px; height: 24px; \">\n  <!-- 14 -->\n  <tr>\n    <td style=\"width: 220px; padding: 1px !important; \"></td>\n    <td style=\"padding: 1px !important; \">{{ doc.other_salary_sides or '&nbsp;' }}</td>\n  </tr>\n</table>\n<table style=\"margin-left: 70px; width: 710px; margin-top: 24px; height: 24px; \">\n  <!-- 15 -->\n  <tr>\n    <td style=\"width: 80px; padding: 1px !important; \"></td>\n    <td style=\"padding: 1px !important; \">{{ doc.remarks or '&nbsp;' }}</td>\n  </tr>\n</table>\n<table style=\"margin-left: 70px; width: 680px; height: 50px; margin-top: 60px; \">\n  <!-- Footer -->\n  <tr>\n    <td style=\"width: 230px; padding: 1px !important; \">{{ doc.get_formatted('modified') }}</td>\n    <td style=\"width: 230px; padding: 1px !important; \"></td>\n    <td style=\"padding: 1px !important; \"></td>\n  </tr>\n</table>", 
